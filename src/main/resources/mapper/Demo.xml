<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="demoDao">

    <select id="findAllAccounts" resultType="java.util.HashMap">
        <![CDATA[
        select * from footprint_account
        ]]>
    </select>

    <select id="getDemoByID" parameterType="int" resultType="me.duelsol.springmvcseed.entity.demo.DemoEntity">
        <![CDATA[
        select * from footprint_demo where id = #{id}
        ]]>
    </select>

</mapper>